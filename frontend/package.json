{
    "name": "sigment-frontend-workspace",
    "private": true,
    "scripts": {
        "dev:board": "cd board && npm run dev",
        "dev:member": "cd member && npm run dev",
        "dev:all": "concurrently \"npm run dev:board\" \"npm run dev:member\"",
        "build:board": "cd board && npm run build",
        "build:member": "cd member && npm run build",
        "build:all": "concurrently \"npm run build:board\" \"npm run build:member\""
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}